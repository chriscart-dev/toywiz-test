{"components/common/subscription-form":"<h5 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h5>\n<p>{{lang 'newsletter.subscribe_intro'}}</p>\n\n<form id=\"newsSignupBottom\" name=\"emailform\" action=\"https://toywiz.us5.list-manage.com/subscribe/post?u=2a5ae42f5a596d5fb587acdb8&amp;id=9190d97493\" method=\"post\" target=\"_blank\" novalidate>\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label u-hiddenVisually\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\" id=\"nl_email\" name=\"EMAIL\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\n\t\t\t\t<div class=\"newsJoin\" onclick=\"document.forms['newsSignupBottom'].submit();\">Join</div>\n            </div>\n        </div>\n    </fieldset>\n</form>\n"}
