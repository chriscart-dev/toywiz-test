{"components/account/orders-list":"<div class=\"sectionTitle\">Your Orders</div>\n<div class=\"ordersBlock\">\n{{#each orders}}\n\t<div class=\"orderRow\">\n\t\t<div class=\"orderHeader\">\n\t\t\t<div class=\"orderHeaderField\">\n\t\t\t\t<label>ORDER PLACED</label><br/>\n\t\t\t\t<span class=\"orderHeaderInfo\">{{date}}</span>\n\t\t\t</div>\n\t\t\t<div class=\"orderHeaderField\">\n\t\t\t\t<label>TOTAL</label><br/>\n\t\t\t\t<span class=\"orderHeaderInfo\">{{total.formatted}}</span>\n\t\t\t</div>\n\t\t\t<div class=\"orderHeaderField\">\n\t\t\t\t<label>STATUS</label><br/>\n\t\t\t\t<span class=\"orderHeaderInfo\"><span class=\"orderStatus order{{status}}\">{{#if this.status \"==\" \"Awaiting Payment\"}}Payment Authorized{{else}}{{this.status}}{{/if}}</span><span class=\"orderHeaderInfo\">\n\t\t\t</div>\n\t\t\t<div class=\"orderHeaderField\">\n\t\t\t\t<label>ORDER #{{id}}</label><br/>\n\t\t\t\t<div class=\"orderHeaderActions\">\n\t\t\t\t\t<a href=\"{{details_url}}\">Order Details</a>\n\t\t\t\t\t<span class=\"orderDivider mobileHideSpan\">|</span>\n\t\t\t\t\t<a href=\"{{#replace \"view_order\" details_url}}print_invoice{{/replace}}\" target=\"_blank\" class=\"mobileHideSpan\">Invoice</a>\n\t\t\t\t\t{{#if this.return_url}}\n\t\t\t\t\t<span class=\"orderDivider\">|</span>\n\t\t\t\t\t<a href=\"{{return_url}}\">Return Items</a>\n\t\t\t\t{{/if}}\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t\t{{#each items}}\n\t\t{{#if quantity \">\" 0}}\n\t\t<div class=\"orderBody\">\n\t\t\t<div class=\"orderItemRow\">\n\t\t\t\t<div class=\"orderItemImage\"><img src=\"{{getImage image 'thumb' (cdn ../theme_settings.default_image_product)}}\"/></div>\n\t\t\t\t<div class=\"orderItemInfo\">\n\t\t\t\t\tx{{quantity}} {{name}}<br/>\n\t\t\t\t\t<span class=\"orderItemPrice\"></span>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<div class=\"linebreak\"></div>\n\t\t</div>\n\t\t{{/if}}\n\t\t{{/each}}\t\t\n\n\t</div>\n{{/each}}\n"}
