{"components/cart/gift-certificate-input":"<div class=\"cart-total-value\">\n\n    <button class=\"gift-certificate-add\">{{lang 'cart.gift_certificates.gift_certificate'}}</button>\n\n    <button class=\"gift-certificate-cancel\" style=\"display: none;\">{{lang 'cart.coupons.cancel'}}</button>\n</div>\n\n<div class=\"cart-form gift-certificate-code\" style=\"display: none;\">\n    <form class=\"form form--hiddenLabels cart-gift-certificate-form\" method=\"post\" action=\"{{urls.cart}}\">\n        <label class=\"form-label\" for=\"couponcode\">{{lang 'cart.gift_certificates.cert_code'}}</label>\n        <input class=\"form-input\" data-error=\"{{lang 'cart.gift_certificates.empty_error'}}\"  id=\"couponcode\" type=\"text\" name=\"certcode\" value=\"\" placeholder=\"{{lang 'cart.gift_certificates.add_cert_code'}}\">\n        <input class=\"button button--primary button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </form>\n</div>\n","components/cart/shipping-estimator":"    <form class=\"form estimator-form\" data-shipping-estimator>\n\t<div id=\"shippingEstimator\" class=\"shipping-estimator u-hiddenVisually\">\n\t\t<div class=\"shippingEstimatorRow\">\n\t\t\t<div class=\"cartImage\">&nbsp;</div>\n\t\t\t<div class=\"cartProduct\">&nbsp;</div>\n\t\t\t<div class=\"cartPrice\"><label>Country:</label></div>\n\t\t\t<div class=\"cartFields\">\n                <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\n                    <option>{{lang 'cart.shipping_estimator.select_a_country'}}</option>\n                    {{#each countries}}\n\t\t\t\t\t\t{{#contains \"Aruba|Australia|Austria|Bahamas|Barbados|Belgium|Bermuda|Canada|Cayman Islands|Costa Rica|Denmark|Finland|France|Germany|Greece|Guam|Hong Kong|Hungary|Ireland|Israel|Italy|Jamaica|Japan|Luxembourg|Netherlands|New Zealand|Norway|Philippines|Poland|Portugal|Puerto Rico|Romania|Singapore|Spain|Sweden|Switzerland|Thailand|Turkey|United Kingdom|United States|Virgin Islands, British|Virgin Islands, U.S.|\" name}}\n\t\t\t\t\t\t\t<option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n\t\t\t\t\t\t\t\t{{name}}\n\t\t\t\t\t\t\t</option>\n\t\t\t\t\t\t{{/contains}}\n                    {{/each}}\n                </select>\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"shippingEstimatorRow\">\n\t\t\t<div class=\"cartImage\">&nbsp;</div>\n\t\t\t<div class=\"cartProduct\">&nbsp;</div>\n\t\t\t<div class=\"cartPrice\"><label>State/Province:</label></div>\n\t\t\t<div class=\"cartFields\">\n                {{#if states.length}}\n                    <select class=\"form-select\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\n                        <option>{{lang 'cart.shipping_estimator.select_a_state'}}</option>\n                        {{#each states}}\n                            <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                                {{name}}\n                            </option>\n                        {{/each}}\n                    </select>\n                {{else}}\n                    <input class=\"form-input\" type=\"text\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\" placeholder=\"{{lang 'cart.shipping_estimator.select_a_state'}}\">\n                {{/if}}\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"shippingEstimatorRow\">\n\t\t\t<div class=\"cartImage\">&nbsp;</div>\n\t\t\t<div class=\"cartProduct\">&nbsp;</div>\n\t\t\t<div class=\"cartPrice\"><label>Zip/Postal:</label></div>\n\t\t\t<div class=\"cartFields\">\n\t\t\t\t<input class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\" placeholder=\"{{lang 'cart.shipping_estimator.zip_postal_code'}}\"/>\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"shippingEstimatorRow\">\n\t\t\t<div class=\"cartImage\">&nbsp;</div>\n\t\t\t<div class=\"cartProduct\">&nbsp;</div>\n\t\t\t<div class=\"cartPrice\"><label>&nbsp;</label></div>\n\t\t\t<div class=\"cartFields cartEstimate\"><input class=\"button button--primary button--small shipping-estimate-submit cartEstimateButton\" type=\"submit\" value=\"View Shipping Options\"/></div>\n\t\t</div>\n\t</div>\n\t\t<div class=\"shippingEstimatorRow\">\n\t\t    <div class=\"shipping-quotes\"></div>\n\t\t</div>\n    </form>\n","components/cart/totals":"<div class=\"cartRow cartSummary\">\n\t<div class=\"cartImage\">&nbsp;</div>\n\t<div class=\"cartProduct\">&nbsp;</div>\n\t<div class=\"cartPrice\">{{lang 'cart.checkout.subtotal'}}:</div>\n\t<div class=\"cartTotal\"><span>{{cart.sub_total.formatted}}</span><div class=\"cartDelete\">&nbsp;</div></div>\n\t<div class=\"linebreak1\">&nbsp;</div>\n</div>\n\n{{#if cart.gift_wrapping_cost.amount}}\n<div class=\"cartRow cartSummary\">\n\t<div class=\"cartImage\">&nbsp;</div>\n\t<div class=\"cartProduct\">&nbsp;</div>\n\t<div class=\"cartPrice\">{{lang 'cart.checkout.gift_wrapping'}}:</div>\n\t<div class=\"cartTotal\"><span>{{cart.gift_wrapping_cost.formatted}}</span><div class=\"cartDelete\">&nbsp;</div></div>\n\t<div class=\"linebreak1\">&nbsp;</div>\n</div>\n{{/if}}\n\n{{#if cart.shipping_handling.show_estimator}}\n<div class=\"cartRow cartSummary\" style=\"display: none;\">\n\t<div class=\"cartImage\">&nbsp;</div>\n\t<div class=\"cartProduct\">&nbsp;</div>\n\t<div class=\"cartPrice\">{{lang 'cart.checkout.shipping'}}:{{#if cart.shipping_handling.shipping_cost}}<br/><span class=\"cartShippingChoice\">{{cart.shipping_handling.provider.description}}</span>{{/if}}</div>\n\t<div class=\"cartTotal\">{{#if cart.shipping_handling.shipping_cost}}<span class=\"cartShippingCost\">{{cart.shipping_handling.shipping_cost.formatted}}</span><br/>{{/if}}<span class=\"cartCalculate\" id=\"cartCalculateLabel\" onclick=\"toggleShippingEstimator();\">{{#if cart.shipping_handling.provider}}Change{{else}}Calculate{{/if}}</span><div class=\"cartDelete\">&nbsp;</div></div>\n\t{{> components/cart/shipping-estimator cart.shipping_handling}}\n</div>\n{{/if}}\n\n\n{{#each cart.taxes}}\n<div class=\"cartRow cartSummary\">\n\t<div class=\"cartImage\">&nbsp;</div>\n\t<div class=\"cartProduct\">&nbsp;</div>\n\t<div class=\"cartPrice\">{{name}}:</div>\n\t<div class=\"cartTotal\"><span>{{cost.formatted}}</span><div class=\"cartDelete\">&nbsp;</div></div>\n\t<div class=\"linebreak1\">&nbsp;</div>\n</div>\n{{/each}}\n\n{{#if cart.discount}}\n<div class=\"cartRow cartSummary\">\n\t<div class=\"cartImage\">&nbsp;</div>\n\t<div class=\"cartProduct\">&nbsp;</div>\n\t<div class=\"cartPrice\">{{lang 'cart.discount'}}:</div>\n\t<div class=\"cartTotal\"><span>{{cart.discount.formatted}}</span><div class=\"cartDelete\">&nbsp;</div></div>\n\t<div class=\"linebreak1\">&nbsp;</div>\n</div>\n{{/if}}\n\n{{#each cart.coupons}}\n<div class=\"cartRow cartSummary\">\n\t<div class=\"cartImage\">&nbsp;</div>\n\t<div class=\"cartProduct\">&nbsp;</div>\n\t<div class=\"cartPrice\">{{lang 'cart.coupons.code_label' code=code}}</div>\n\t<div class=\"cartTotal\"><span>{{discount.formatted}}</span><a href=\"{{remove_url}}\"><div class=\"cartDelete\"><img src=\"data:image/png;base64,\"/></div></a></div>\n\t<div class=\"linebreak1\">&nbsp;</div>\n</div>\n{{/each}}\n\n{{#each cart.gift_certificates}}\n<div class=\"cartRow cartSummary\">\n\t<div class=\"cartImage\">&nbsp;</div>\n\t<div class=\"cartProduct\">&nbsp;</div>\n\t<div class=\"cartPrice\">{{lang 'cart.gift_certificates.code_label' code=code}}</div>\n\t<div class=\"cartTotal\"><span>{{remaining.formatted}} | {{used.formatted}}</span><a href=\"{{remove_url}}\"><div class=\"cartDelete\"><img src=\"data:image/png;base64,\"/></div></a></div>\n\t<div class=\"linebreak1\">&nbsp;</div>\n</div>\n{{/each}}\n\n{{#if cart.grand_total.value \"==\" cart.sub_total.value}}\n{{else}}\n<div class=\"cartRow cartSummary\">\n\t<div class=\"cartImage\">&nbsp;</div>\n\t<div class=\"cartProduct\">&nbsp;</div>\n\t<div class=\"cartPrice\">Total:</div>\n\t<div class=\"cartTotal\"><span>{{cart.grand_total.formatted}}</span><div class=\"cartDelete\">&nbsp;</div></div>\n\t<div class=\"linebreak1\">&nbsp;</div>\n</div>\n{{/if}}\n\n<script>\nfunction toggleShippingEstimator(){\n\tif($(\"#shippingEstimator\").css('display') == 'none'){\n\t\t$(\"#shippingEstimator\").css('display','block');\n\t\t$(\"#cartCalculateLabel\").html('Cancel');\n\t} else {\n\t\t$(\"#shippingEstimator\").css('display','none');\n\t\t$(\"#cartCalculateLabel\").html('Calculate');\n\t}\n}\n\nvar freeThresh = 40;\nvar totalQTY = 0;\nvar totalPrice = 0;\nvar totalItems = 0;\nvar singleCount = 0;\n{{#each cart.items}}\ntotalQTY = totalQTY + {{quantity}};\ntotalPrice = totalPrice + {{price.value}}*{{quantity}};\ntotalItems = totalItems + 1;\n{{#each custom_fields}}\n{{#if name '==' \"Product Type\"}}\n{{#if value '==' \"Single Card\"}}\nsingleCount = singleCount + 1;\n{{/if}}\n{{/if}}\n{{/each}}\n{{/each}}\n\nif(singleCount > 0 && singleCount == totalItems && totalPrice < freeThresh){\n\tvar totalNeeded = freeThresh - totalPrice;\n\tvar msg = 'You need just $' + totalNeeded.toFixed(2) + ' more worth of single cards to qualify for free U.S. shipping!';\n\t$(\"#shippingMessage\").html(msg);\n\t$(\"#shippingMessage\").css('display','block');\n} else if(singleCount > 0 && singleCount == totalItems && totalPrice >= freeThresh){\n\tvar msg = 'Congratulations! You qualify for free shipping in the U.S. for $' + freeThresh + ' or more worth of single cards!';\n\t$(\"#shippingMessage\").html(msg);\n\t$(\"#shippingMessage\").css('display','block');\n} else {\n\t$(\"#shippingMessage\").css('display','none');\n}\n</script>\n\n\n\n\n<!---\n    <li class=\"cart-total\">\n        <div class=\"cart-total-label\">\n            <strong>{{lang 'cart.gift_certificates.gift_certificate'}}:</strong>\n        </div>\n        {{> components/cart/gift-certificate-input}}\n    </li>\n--->\n"}
