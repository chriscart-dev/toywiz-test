{"components/faceted-search/facets/multi":"{{#if items.length \">\" \"1\"}}\n{{#if title \"==\" \"Filter\"}}\n<div id=\"filterFilter\"></div>\n{{else}}\n<div id=\"filter{{facet}}\" class=\"filterBlockXXX\" style=\"display: block;\">\n<div class=\"accordion-block\">\n\t<div class=\"filterHeader\"><span>{{title}}</span><div id=\"\" class=\"fiterHeaderTriangle fiterHeaderTriangle-rotate\">&#9654;</div></div>\n    <div class=\"accordion-navigation toggleLink {{#unless ../start_collapsed }} is-open {{/unless}}\" role=\"button\" data-collapsible=\"#facetedSearch-content--{{ facet }}\" onclick=\"toggleFilters('{{title}}','{{items.length}}');\">\n        <h5 class=\"accordion-title\">\n        \t<div id=\"accordion-triangle-{{title}}\" class=\"accordion-triangle\">&#9654;</div>\n        </h5>\n\n        <div class=\"accordion-navigation-actions\">\n            {{#if selected}}\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet rel=\"nofollow\">{{lang 'search.faceted.clear'}}</a>\n\t\t\t\t<script>\n\t\t\t\tshowFilter.push('{{facet}}');\n\t\t\t\tfilterCount.push('{{items.length}}');\n\t\t\t\t</script>\n            {{/if}}\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\n                <use xlink:href=\"#icon-add\" />\n            </svg>\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\n                <use xlink:href=\"#icon-remove\" />\n            </svg>\n        </div>\n    </div>\n\n    <div class=\"filterBlock\" style=\"display: block;\" id=\"accordion-content-{{title}}\" class=\"accordion-content {{#unless ../start_collapsed }} is-open {{/unless}}\">\n        <ul id=\"facetedSearch-navList--{{ facet }}\" data-facet=\"{{facet}}\" class=\"navList\" data-has-more-results=\"{{ has_more_results }}\">\n            {{#each items}}\n                <li class=\"navList-item\">\n                    <a\n                        href=\"{{ url }}\"\n                        class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}}\"\n                        data-faceted-search-facet rel=\"nofollow\">\n                        {{ title }}\n                            <span>({{ count }})</span>\n                        {{#if ../show_product_counts}}\n                        {{/if}}\n\n                        <span class=\"navList-action-close\" aria-hidden=\"true\">\n                            <svg class=\"icon\">\n                                <use xlink:href=\"#icon-close\"/>\n                            </svg>\n                        </span>\n                    </a>\n                </li>\n            {{/each}}\n        </ul>\n\n        {{#if show_more_toggle}}\n            <a href=\"#facetedSearch-navList--{{ facet }}\" role=\"button\" class=\"toggleLink\" rel=\"nofollow\">\n                <span class=\"toggleLink-text toggleLink-text--off\">{{lang 'search.faceted.show-more'}}</span>\n            </a>\n        {{/if}}\n    </div>\n</div>\n</div>\n{{/if}}\n{{/if}}\n{{#if title \"==\" \"Filter\"}}\n<script>$(document).ready(fireWhenReady('{{title}}','closed'));</script>\n{{/if}}\n"}
